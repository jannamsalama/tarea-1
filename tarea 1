package jannam;

import java.util.Scanner;

/**
 *
 * @author SALA
 */
public class Jannam {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        
      
    Scanner objetoentrada=new Scanner (System.in);
    int lista[]=new int[5];

    for (int i=0; i<lista.length; i++) {
        System.out.println ("Por favor ingrese un numero para su lista");
        lista [i]=objetoentrada.nextInt();

        }

    for (int elemento: lista) {
        System.out.println ("Su lista es la siguiente "+elemento);
        }
    }
}  
        
        
        
